{
    "version": 2,
    "builds": [
        {
            "src": "index.py",
            "use": "@vercel/python",
            "config": {
                "runtime": "python3.10"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "index.py"
        }
    ],
    "build": {
        "env": {
            "PYTHON_VERSION": "3.10"
        },
        "commands": [
            "pip install --upgrade pip setuptools wheel",
            "pip install --no-cache-dir -r backend/requirements.txt"
        ]
    }
}
